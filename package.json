    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "nest-commander": "^3.6.1",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.0",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.14"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.0",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cron": "^2.0.1",
    "@types/crypto-js": "^4.1.1",
    "@types/i18n": "^0.13.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/qrcode": "^1.5.0",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^@configs/(.*)$": "<rootDir>/configs/$1",
      "^@base/(.*)$": "<rootDir>/base/$1",
      "^@validators/(.*)$": "<rootDir>/validators/$1",
      "^@modules/(.*)$": "<rootDir>/modules/$1",
      "^@exceptions/(.*)$": "<rootDir>/exceptions/$1",
      "^@decorators/(.*)$": "<rootDir>/decorators/$1",
      "^@enums/(.*)$": "<rootDir>/enums/$1",
      "^@constants/(.*)$": "<rootDir>/constants/$1",
      "^@entities/(.*)$": "<rootDir>/entities/$1",
      "^@guard/(.*)$": "<rootDir>/guard/$1",
      "^@utils/(.*)$": "<rootDir>/utils/$1",
      "^@libs/(.*)$": "<rootDir>/libs/$1",
      "^@src/(.*)$": "<rootDir>/$1"
    }
  }
}