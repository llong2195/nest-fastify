{
  "name": "nest-fastify-llong",
  "version": "0.0.1",
  "description": "",
  "author": "nduylong9501@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "lint-staged": "lint-staged --allow-empty --relative --config .lintstagedrc.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "npm run typeorm -- migration:create",
    "typeorm:up": "npm run typeorm -- --dataSource=./data-source.ts migration:run",
    "typeorm:down": "npm run typeorm -- --dataSource=./data-source.ts migration:revert",
    "typeorm:show": "npm run typeorm -- --dataSource=./data-source.ts migration:show"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.2.0",
    "@fastify/static": "^6.12.0",
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.3.7",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.5",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/node": "^7.108.0",
    "@sentry/tracing": "^7.108.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.0.3",
    "content-disposition": "^0.5.4",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
