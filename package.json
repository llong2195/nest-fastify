{
  "name": "nest-fastify-llong",
  "version": "0.0.1",
  "description": "",
  "author": "nduylong9501@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky",
    "lint-staged": "lint-staged --allow-empty --relative --config .lintstagedrc.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "npm run typeorm -- migration:create",
    "typeorm:up": "npm run typeorm -- --dataSource=./data-source.ts migration:run",
    "typeorm:down": "npm run typeorm -- --dataSource=./data-source.ts migration:revert",
    "typeorm:show": "npm run typeorm -- --dataSource=./data-source.ts migration:show"
  },
  "dependencies": {
    "@fastify/helmet": "^12.0.1",
    "@fastify/multipart": "8.3.0",
    "@fastify/static": "^7.0.4",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^10.2.3",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-fastify": "^10.4.15",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/throttler": "^6.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/node": "^8.46.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "content-disposition": "^0.5.4",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "fastify": "^4.28.1",
    "geolib": "^3.3.4",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash.camelcase": "^4.3.0",
    "mime-types": "^2.1.35",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/bcrypt": "^5.0.2",
    "@types/content-disposition": "^0.5.8",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.10.2",
    "@types/qrcode": "^1.5.5",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^@/(.*)$": "<rootDir>/$1"
    }
  }
}
