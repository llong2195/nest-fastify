{
  "name": "nest-starter-llong",
  "version": "0.0.1",
  "description": "",
  "author": "nduylong9501@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "npm run typeorm -- migration:create",
    "typeorm:up": "npm run typeorm -- --dataSource=./data-source.ts migration:run",
    "typeorm:down": "npm run typeorm -- --dataSource=./data-source.ts migration:revert",
    "typeorm:show": "npm run typeorm -- --dataSource=./data-source.ts migration:show",
    "commander": "ts-node -r tsconfig-paths/register ./src/commander.ts",
    "seed:user": "npm run commander seed:user"
  },
  "dependencies": {
    "@fastify/helmet": "^10.1.0",
    "@fastify/multipart": "^7.6.0",
    "@fastify/static": "^6.10.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-fastify": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/node": "^7.47.0",
    "@sentry/tracing": "^7.47.0",
    "aws-sdk": "^2.1357.0",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
    "change-case": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.35.0",
    "content-disposition": "^0.5.4",
    "crypto-js": "^4.1.1",
