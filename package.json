{
  "name": "nest-fastify",
  "version": "0.0.1",
  "description": "",
  "author": "nduylong9501@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky || true",
    "lint-staged": "lint-staged --allow-empty --relative --config .lintstagedrc.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "npm run typeorm -- migration:create",
    "typeorm:up": "npm run typeorm -- --dataSource=./data-source.ts migration:run",
    "typeorm:down": "npm run typeorm -- --dataSource=./data-source.ts migration:revert",
    "typeorm:show": "npm run typeorm -- --dataSource=./data-source.ts migration:show"
  },
  "dependencies": {
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.2.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-fastify": "^11.1.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@sentry/node": "^9.22.0",
    "@supercharge/request-ip": "^1.2.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.53.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "content-disposition": "^0.5.4",
    "crypto-js": "^4.2.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "geolib": "^3.3.4",
    "handlebars": "^4.7.8",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.6.1",
    "mime-types": "^3.0.1",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.24",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.1",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.29",
    "@types/content-disposition": "^0.5.8",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.17",
    "@types/luxon": "^3.6.2",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.15.21",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^@/(.*)$": "<rootDir>/$1"
    }
  }
}
