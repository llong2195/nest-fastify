    "sharp": "^0.31.3",
    "slugify": "^1.6.5",
    "typeorm": "0.3.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.8",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cron": "^2.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/i18n": "^0.13.6",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
